<style>
    .flx{
        display: flex;
        flex-wrap: wrap;
        justify-content: space-around;
    }
    .flx .event{
        width: 30%;
        margin: 40px auto;
        .icons{
            width: 100px;
            height: 100px;
            background: #FF6700;
            border-radius: 50%;
            margin: 8px auto;
            svg{
                width: 70%;
                height: 70%;
                margin: 15%;
                color: #fff;
            }
        }
        .head{
            font-size: 36px;
            font-weight: bold;
            font-family: 'Times New Roman', Times, serif;
            color: #FF6700;
        }
        .body{
            font-size: 18px;
            color: #fff
        }
    }
    #onStageEvents, #offStageEvents, #fieldEvents{
        display: none;
    }
    @media only screen and (max-width: 900px), only screen and (min-width: 900px) and (max-width: 1200px){
        .flx .event{
            width: 90%;
        }
    }
</style>

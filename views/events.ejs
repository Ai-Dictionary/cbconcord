<script>
    document.getElementById('header').innerHTML = document.getElementById('header').textContent;
    document.getElementById('rs-footer').innerHTML = document.getElementById('rs-footer').textContent;
    function displayEvent(event){
        let event_list = ['onStageEvents', 'offStageEvents', 'fieldEvents'];
        if(event=='all'||event===undefined||event===null){
            for(let i=0; i<event_list.length; i++){
                document.getElementById(event_list[i]).style.display = "block";
            }
        }else{
            for(let i=0; i<event_list.length; i++){
                document.getElementById(event_list[i]).style.display = "none";
                if(event_list[i]==event){
                    document.getElementById(event_list[i]).style.display = "block";
                }
            }
        }
    }
    const urlParams = new URLSearchParams(window.location.search);
    let searchParam = urlParams.get('event');
    if(searchParam==null) searchParam='all';
    displayEvent(searchParam);
</script>

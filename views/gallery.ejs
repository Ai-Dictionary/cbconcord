<script>
    document.getElementById('header').innerHTML = document.getElementById('header').textContent;
    document.getElementById('rs-footer').innerHTML = document.getElementById('rs-footer').textContent;
    document.querySelector('.previewImage-back').innerHTML = document.querySelector('.previewImage-back').textContent;
    previewImagesListener();
    const urlParams = new URLSearchParams(window.location.search);
    const searchParam = urlParams.get('imageReview');
    if(searchParam!=null){
	    previewImages((searchParam*1));
    }
    function setGallary(){
        let imgbin = document.querySelectorAll('.crop-image');
        for(let i=0; i<imgbin.length; i++){
            imgbin[i].style.backgroundImage = `url(${imgbin[i].childNodes[1].src})`;
        }
    }
    setGallary();
</script>
